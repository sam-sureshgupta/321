<apex:page standardController="Account">
    <apex:pageBlock >
    	<apex:pageBlockTable value="{!Account.cases}" var="a"> 
            <apex:column value="{!a.subject}" colspan="10"/>
            <apex:column value="{!a.priority}" />
            <apex:column value="{!a.status}" />
            <apex:column value="{!a.origin}" />
        </apex:pageBlockTable>
    </apex:pageBlock>
</apex:page>